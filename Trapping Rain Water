class Solution {
    public int trap(int[] h) {
        int l=0,r=h.length-1, L=0,R=0, ans=0;
        while(l<r){
            if(h[l]<=h[r]){
                L=Math.max(L,h[l]); ans+=L-h[l++]; 
            }else{
                R=Math.max(R,h[r]); ans+=R-h[r--];
            }
        }
        return ans;
    }
}
