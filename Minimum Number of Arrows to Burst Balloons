class Solution {
    public int findMinArrowShots(int[][] p) {
        Arrays.sort(p, (a,b)-> Integer.compare(a[1], b[1]));
        int arrows=0, end=Integer.MIN_VALUE;
        for(int[] x: p){
            if(arrows==0 || x[0]>end){
                arrows++; end=x[1];
            }
        }
        return arrows;
    }
}
